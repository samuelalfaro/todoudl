            <WordsStyle name="NUMBERS" fgColor="53A0B0" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="309EB1" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="00C632" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="FF1A1A" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="80FF00" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="FF80C0" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS6" fgColor="F2AD00" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
