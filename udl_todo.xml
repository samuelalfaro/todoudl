            <WordsStyle name="DEFAULT" fgColor="BEBE97" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" fgColor="829564" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
