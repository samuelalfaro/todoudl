            <WordsStyle name="FOLDER IN CODE2" fgColor="FFFF00" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="829564" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="48A4FF" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="DELIMITERS2" fgColor="FF8000" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
